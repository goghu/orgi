<p id="notice"><%= notice %></p>

<h1>Categoria</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Icono</th>
      <th>Nombre</th>
      <th>Tipo</th>
      <th>Movimiento</th>
      <th>Descripcion</th>
      <th>Borrado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @categoria.each do |categorium| %>
      <tr>
        <td><%= categorium.user_id %></td>
        <td><%= categorium.icono_id %></td>
        <td><%= categorium.nombre %></td>
        <td><%= categorium.tipo %></td>
        <td><%= categorium.movimiento %></td>
        <td><%= categorium.descripcion %></td>
        <td><%= categorium.borrado %></td>
        <td><%= link_to 'Show', categorium %></td>
        <td><%= link_to 'Edit', edit_categorium_path(categorium) %></td>
        <td><%= link_to 'Destroy', categorium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Categorium', new_categorium_path %>
